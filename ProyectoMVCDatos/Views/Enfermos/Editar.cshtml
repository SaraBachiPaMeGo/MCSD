
@{
    ViewBag.Title = "Editar";
}

<h2>Editar</h2>

<form class="form-group" method="post">
    <hr />
    <label>Inscripción</label>
    <input type="number" class="form-control" name="insc" value="@Model.INSCRIPCION" readonly/>
    <label>Apellido</label>
    <input type="text" class="form-control" name="apellido" value="@Model.APELLIDO" />
    <label>Dirección</label>
    <input type="text" class="form-control" name="direccion" value="@Model.DIRECCION" />
    <label>Fecha de Nacimiento</label>
    <input type="datetime" class="form-control" name="fecha_nac" value="@Model.FECHA_NAC" />
    <label>Sexo</label>
    <input type="text" class="form-control" name="s" value="@Model.S" />
    <label>Número de la SS</label>
    <input type="text" class="form-control" name="nss" value="@Model.NSS" />
    <hr />
    <button type="submit" class="btn-info">Editar enfermo</button>
</form>